{
  "name": "passphrase-forge",
  "version": "2025.3.0",
  "private": true,
  "bin": {
    "passphrase": "./bin/passphrase.js"
  },
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ai": "RUN_AI_TESTS=true vitest run app/passphrase/__tests__/ai-quality/",
    "check": "npm run typecheck && npm run lint && npm run prettier:check && npm run test",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "vercel:env": "vercel env pull",
    "cli": "tsx bin/passphrase-cli.ts",
    "cli:basic": "tsx bin/passphrase-cli.ts --generator basic"
  },
  "dependencies": {
    "@ai-sdk/gateway": "^2.0.18",
    "@ai-sdk/openai": "^2.0.77",
    "@next/bundle-analyzer": "16.0.7",
    "@opentelemetry/api": "^1.9.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/otel": "^2.1.0",
    "ai": "^5.0.108",
    "commander": "^14.0.2",
    "cookies-next": "^6.1.1",
    "flowbite-react": "^0.12.13",
    "idb": "^8.0.3",
    "negotiator": "^1.0.0",
    "next": "16.0.10",
    "next-intl": "^4.5.8",
    "next-mdx-remote": "^5.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "swr": "^2.3.7",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.15",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.2.0",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
